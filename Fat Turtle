function setup() {
  createCanvas(windowWidth,windowHeight);
}

function draw() {
  background("white");
  text("X = " + mouseX + " | " + "Y = " + mouseY,50,50);
ellipse(200,200,200,200);//body
ellipse(144,303,30,30);//front left foot
ellipse(255,303,30,30);//front right foot
 //ellipse(100,225,60,60);//head
ellipse(168,305,15,15); 
ellipse(215,310,15,15);

line(100,192,297,192);//shell 
rect(125,160,50,25);
rect(220,150,50,25);
  

rotate(PI/9.0);
ellipse(99,190,30,60);//neck

}
